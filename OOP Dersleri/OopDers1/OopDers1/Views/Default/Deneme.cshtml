@model List<OopDers1.Ornekler.Sehirler>;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Deneme</title>

    	<link href="~/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />
	<link href="~/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />
	<link href="~/twitter-bootstrap/css/bootstrap.rtl.css" rel="stylesheet" />
	<link href="~/twitter-bootstrap/css/bootstrap.rtl.min.css" rel="stylesheet" />


</head>
<body>

    <table class="table table-bordered" >
        <tr>
            <th>ID</th>
            <th>ŞEHİR AD</th>
            <th>ÜLKE</th>
            <th>NUFÜS</th>
            <th>Bayrak Rengi 1</th>
            <th>Bayrak Rengi 2</th>
            <th>Bayrak Rengi 3</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.SehirAd</td>
                <td>@item.Ulke</td>
                <td>@item.Nufus</td>
                <td>@item.Renk1</td>
                <td>@item.Renk2</td>
                <td>@item.Renk3</td>
            </tr>
        }
    </table>



</body>
</html>
